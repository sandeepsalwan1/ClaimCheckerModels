{
  "name": "news-fact-checker",
  "version": "1.0.0",
  "description": "AI-powered Chrome extension for fact-checking news articles",
  "main": "background.js",
  "type": "module",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "test": "node run-tests.js",
    "test:svm": "node test-svm.js",
    "test:all": "node run-tests.js"
  },
  "dependencies": {
    "@tensorflow/tfjs": "3.21.0",
    "@tensorflow/tfjs-core": "3.21.0",
    "@tensorflow/tfjs-converter": "3.21.0",
    "@tensorflow/tfjs-backend-webgl": "3.21.0",
    "@tensorflow-models/universal-sentence-encoder": "1.3.3",
    "@tensorflow-models/toxicity": "1.2.2",
    "ml-svm": "^2.1.2",
    "ml-matrix": "^6.10.4",
    "ml-dataset-iris": "^1.2.1",
    "ml-pca": "^4.0.2",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "copy-webpack-plugin": "^11.0.0",
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "babel-loader": "^9.1.3"
  }
} 